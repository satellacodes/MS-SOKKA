{
  "headers": [
    {
      "source": "/assets/(.*).css",
      "headers": [
        { "key": "Content-Type", "value": "text/css" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],

  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
